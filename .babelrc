{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "jsxPragma": "h"
      }
    ],
    [
      "@parcel/babel-preset-env",
      {
        "loose": true,
        "bugfixes": true
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "loose": true,
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
